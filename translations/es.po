# ERROR_EMPTY_PATH = 1
msgid "Empty file path provided"
msgstr "La ruta de archivo proporcionada está vacía"

# ERROR_FILE_NOT_FOUND = 2
msgid "File %filename not found"
msgstr "No se puede encontrar el archivo %filename"

# ERROR_FILE_NOT_READABLE = 3
msgid "File %filename is not readable"
msgstr "El archivo %filename no es legible"

# ERROR_FILE_NOT_WRITABLE = 4;
msgid "File %filename is not writable"
msgstr "El archivo %filename no es escribible"

# ERROR_READ_FAILED = 5
msgid "The file %filename could not be read"
msgstr "El archivo %filename no pudo ser leido"

# ERROR_EMPTY_FILE = 6
msgid "The file %filename is empty"
msgstr "El archivo %filename está vacío"

# ERROR_NO_YAML_DATA = 7
