msgstr "El archivo %filename no contiene datos YAML"

# ERROR_WRITE_FAILED = 8
msgid "The file %filename cannot be written."
msgstr "El archivo %filename no pudo ser escrito."

#ERROR_NO_DATA = 9
msgid "No data provided"
msgstr "No se han proporcionado datos"

# ERROR_VALIDATION_FAILED = 10
msgid "YAML validation failed"
msgstr "Ha fallado la validación del YAML"

msgid "Loading"
msgstr "Cargando"

msgid "Saving"
msgstr "Guardando"

msgid "@stage_action the file: %filename: "
msgstr "@stage_action el fichero: %filename: "

# ERROR_SCALAR_VALUE = 11
msgid "Scalar value"
msgstr "Valor escalar"

msgid "Scalar values ​​are not evaluated"
msgstr "Los valores escalares no son evaluados"

# ERROR_PARSE_FAILED = 12
msgid "Conversion to yaml failed"
msgstr "La conversión a yaml ha fallado"

# ERROR_UNSUPPORTED_TYPE = 13
msgid "Unsupported input type"
msgstr "Los datos recibido no son soportados"

# ERROR_NO_DATA = 14
msgid "Empty input"
msgstr "Entrada de datos vacía"

msgid "No data to validate"
msgstr "No hay datos para validar"

